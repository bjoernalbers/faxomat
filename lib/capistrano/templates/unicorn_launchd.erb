<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>de.faxomat.unicorn</string>
	<key>ProgramArguments</key>
	<array>
		<string>bundle</string>
		<string>exec</string>
		<string>unicorn</string>
		<string>--config-file</string>
		<string>config/unicorn.rb</string>
	</array>
	<key>EnvironmentVariables</key>
	<dict>
		<key>PATH</key>
		<string>/usr/local/bin:/usr/local/opt/ruby/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
		<key>RAILS_ENV</key>
		<string>production</string>
	</dict>
        <key>RunAtLoad</key>
        <true/>
        <key>KeepAlive</key>
        <true/>
        <key>WorkingDirectory</key>
        <string>/Users/rgp/faxomat/current</string>
        <key>UserName</key>
        <string>rgp</string>
        <key>GroupName</key>
        <string>staff</string>
</dict>
</plist>
