<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>de.faxomat.delete_old_exports</string>
  <key>ProgramArguments</key>
  <array>
    <string>bundle</string>
    <string>exec</string>
    <string>rake</string>
    <string>faxomat:delete_old_exports</string>
  </array>
  <key>EnvironmentVariables</key>
  <dict>
    <key>PATH</key>
    <string>/usr/local/bin:/usr/local/opt/ruby/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    <key>RAILS_ENV</key>
    <string>production</string>
  </dict>
  <key>RunAtLoad</key>
  <true/>
  <key>StartCalendarInterval</key>
  <dict>
    <key>Hour</key>
    <integer>20</integer>
    <key>Minute</key>
    <integer>15</integer>
  </dict>
  <key>WorkingDirectory</key>
  <string><%= current_path %></string>
  <key>UserName</key>
  <string><%= fetch(:user) %></string>
  <key>StandardOutPath</key>
  <string><%= current_path.join('log', 'delete_old_exports.log') %></string>
  <key>StandardErrorPath</key>
  <string><%= current_path.join('log', 'delete_old_exports.log') %></string>
</dict>
</plist>
